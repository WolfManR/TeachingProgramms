<Window x:Class="MailSender_WPFTest.Views.SendEndWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MailSender_WPFTest.Views"
        mc:Ignorable="d"
        Title="SendEndWindow" Height="175" Width="400" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" BorderBrush="{StaticResource ControlBorderBrush}" BorderThickness="2">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <Label Name="ISendEnd" Content="Работа завершена" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36" Foreground="{StaticResource NormalBrush}"/>
        <Button x:Name="btnOK" Grid.Row="1" Content="OK" Margin="0,8,0,20" Width="80" Height="32" Click="btnOK_Click"/>
    </Grid>
</Window>
